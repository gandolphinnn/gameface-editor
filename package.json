{
	"name": "gameface-in-game-editor",
	"version": "1.0.0",
	"description": "A comprehensive in-game editor built with Gameface UI technology for real-time 3D scene editing and asset management",
	"main": "dist/main.js",
	"scripts": {
		"lint": "eslint src/**/*.ts",
		"format": "prettier --write \"**/*.{ts,js,css,html,md}\"",
		"watch": "tsc --watch",
		"serve": "live-server --port=8080 --no-browser",
		"play": "D:\\Plugins\\CoherentSample\\Apps\\Player.exe --url=http://localhost:8080"
	},
	"keywords": [
		"gameface",
		"ui",
		"editor",
		"game-development",
		"3d",
		"scene-editor",
		"in-game-editor",
		"html5",
		"javascript",
		"css3"
	],
	"author": "Gameface Editor Team",
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^20.11.0",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"http-server": "^14.1.1",
		"live-server": "^1.2.2",
		"prettier": "^3.2.5",
		"typescript": "^5.3.0"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/your-username/gameface-in-game-editor.git"
	},
	"bugs": {
		"url": "https://github.com/your-username/gameface-in-game-editor/issues"
	},
	"homepage": "https://github.com/your-username/gameface-in-game-editor#readme",
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"gameface": {
		"version": ">=2.0.0",
		"components": [
			"data-binding",
			"ui-components",
			"engine-integration"
		],
		"features": [
			"3d-viewport",
			"asset-management",
			"scene-hierarchy",
			"property-editing",
			"tool-system"
		]
	}
}